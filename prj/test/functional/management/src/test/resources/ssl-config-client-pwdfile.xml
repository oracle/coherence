<ssl>
  <identity-manager>
    <key-store>
      <url>file:client.jks</url>
      <password-provider>
        <class-name>com.tangosol.net.FileBasedPasswordProvider</class-name>
        <init-params>
          <init-param>
            <param-name>sFile</param-name>
            <param-type>String</param-type>
            <param-value>storepass.txt</param-value>
          </init-param>
        </init-params>
      </password-provider>
    </key-store>
    <password-provider>
      <class-name>com.tangosol.net.FileBasedPasswordProvider</class-name>
      <init-params>
        <init-param>
          <param-name>sFile</param-name>
          <param-type>String</param-type>
          <param-value>keypass.txt</param-value>
        </init-param>
      </init-params>
    </password-provider>
  </identity-manager>

  <trust-manager>
    <key-store>
      <url>file:trust-client.jks</url>
      <password-provider>
        <class-name>com.tangosol.net.FileBasedPasswordProvider</class-name>
        <init-params>
          <init-param>
            <param-name>sFile</param-name>
            <param-type>String</param-type>
            <param-value>trustpass.txt</param-value>
          </init-param>
        </init-params>
      </password-provider>
    </key-store>
  </trust-manager>
</ssl>
