<cache-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xmlns="http://xmlns.oracle.com/coherence/coherence-cache-config"
              xsi:schemaLocation="http://xmlns.oracle.com/coherence/coherence-cache-config coherence-cache-config.xsd">

  <caching-scheme-mapping>
    <cache-mapping>
      <cache-name>dist-*</cache-name>
      <scheme-name>dist-default</scheme-name>
    </cache-mapping>
    <cache-mapping>
      <cache-name>tracing-*</cache-name>
      <scheme-name>dist-tracing</scheme-name>
    </cache-mapping>
    <cache-mapping>
      <cache-name>test-expiry</cache-name>
      <scheme-name>dist-expiry</scheme-name>
    </cache-mapping>
    <cache-mapping>
      <cache-name>test-no-expiry</cache-name>
      <scheme-name>dist-no-expiry</scheme-name>
    </cache-mapping>
    <cache-mapping>
      <cache-name>test-coh16231</cache-name>
      <scheme-name>dist-coh16231</scheme-name>
    </cache-mapping>
    <cache-mapping>
      <cache-name>repl-*</cache-name>
      <scheme-name>repl-default</scheme-name>
    </cache-mapping>
  </caching-scheme-mapping>

  <caching-schemes>
    <distributed-scheme>
      <scheme-name>dist-default</scheme-name>
      <service-name>CQCTestService</service-name>
      <backing-map-scheme>
        <local-scheme/>
      </backing-map-scheme>
      <autostart>true</autostart>
    </distributed-scheme>

    <distributed-scheme>
      <scheme-name>dist-expiry</scheme-name>
      <service-name>DistributedExpiry</service-name>
      <key-associator>
        <class-name>cache.EvictionTests$Associator</class-name>
      </key-associator>
      <backing-map-scheme>
        <sliding-expiry system-property="test.expiry.sliding">false</sliding-expiry>
        <local-scheme>
          <expiry-delay>3s</expiry-delay>
        </local-scheme>
      </backing-map-scheme>
    </distributed-scheme>

    <distributed-scheme>
      <scheme-name>dist-no-expiry</scheme-name>
      <service-name>DistributedNoExpiry</service-name>
      <key-associator>
        <class-name>cache.EvictionTests$Associator</class-name>
      </key-associator>
      <backing-map-scheme>
        <local-scheme>
        </local-scheme>
      </backing-map-scheme>
    </distributed-scheme>

    <distributed-scheme>
      <scheme-name>dist-coh16231</scheme-name>
      <service-name>DistributedCoh16231</service-name>
      <backing-map-scheme>
        <local-scheme>
          <high-units>10000</high-units>
        </local-scheme>
      </backing-map-scheme>
      <autostart>true</autostart>
    </distributed-scheme>

    <distributed-scheme>
      <scheme-name>dist-tracing</scheme-name>
      <service-name>DistributedCacheTracing</service-name>
      <local-storage system-property="storage.enabled">true</local-storage>
      <!--<backing-map-scheme>
        <read-write-backing-map-scheme>
          <internal-cache-scheme>
            <local-scheme></local-scheme>
          </internal-cache-scheme>
          <cachestore-scheme>
            <class-scheme>
              <class-name>common.TestCacheStore</class-name>
            </class-scheme>
          </cachestore-scheme>
          <write-delay-seconds>1</write-delay-seconds>
        </read-write-backing-map-scheme>
      </backing-map-scheme>
      <persistence>
        <environment>default-live</environment>
      </persistence> -->
      <autostart>true</autostart>
    </distributed-scheme>

    <replicated-scheme>
      <scheme-name>repl-default</scheme-name>
      <lease-granularity>member</lease-granularity>
      <backing-map-scheme>
        <local-scheme/>
      </backing-map-scheme>
      <autostart>true</autostart>
    </replicated-scheme>
</caching-schemes>
</cache-config>
