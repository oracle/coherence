<coherence  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns="http://xmlns.oracle.com/coherence/coherence-operational-config"
            xsi:schemaLocation="http://xmlns.oracle.com/coherence/coherence-operational-config coherence-operational-config.xsd"
            xml-override="common/tangosol-coherence-override.xml">

  <cache-factory-builder-config>
    <class-name system-property="tangosol.coherence.cachefactorybuilder">com.tangosol.net.ScopedCacheFactoryBuilder</class-name>
    <scope-resolver>
      <class-name system-property="tangosol.coherence.scoperesolver">cache.CacheFactoryBuilderTests$ScopedLoaderResolver</class-name>
    </scope-resolver>
  </cache-factory-builder-config>
</coherence>
