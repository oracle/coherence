<socket-provider>
  <ssl>
    <identity-manager>
      <key-store>
        <url>file:server.jks</url>
        <password>password</password>
      </key-store>
      <password>private</password>
    </identity-manager>

    <trust-manager>
      <key-store>
        <url>file:trust.jks</url>
        <password>password</password>
      </key-store>
    </trust-manager>
    <!-- no verifier so use the default -->
    <hostname-verifier/>
  </ssl>
</socket-provider>
