<socket-provider>
  <ssl>
    <identity-manager>
      <key-store>
        <url>file:server.jks</url>
        <password>password</password>
      </key-store>
      <password>private</password>
    </identity-manager>

    <trust-manager>
      <key-store>
        <url>file:trust.jks</url>
        <password>password</password>
      </key-store>
    </trust-manager>

    <hostname-verifier>
      <class-name>ssl.CustomHostnameVerifier</class-name>
      <init-params>
        <init-param>
          <param-type>boolean</param-type>
          <param-value>false</param-value>
        </init-param>
      </init-params>
    </hostname-verifier>
  </ssl>
</socket-provider>
