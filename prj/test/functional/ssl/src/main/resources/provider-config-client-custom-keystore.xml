<socket-provider>
  <ssl>
    <identity-manager>
      <key-store>
        <key-store-loader>
          <class-name>ssl.CustomKeyStoreLoader</class-name>
          <init-params>
            <init-param>
              <param-type>string</param-type>
              <param-value system-property="coherence.security.client.key">file:client.jks</param-value>
            </init-param>
          </init-params>
        </key-store-loader>
        <password>password</password>
      </key-store>
      <password>private</password>
    </identity-manager>
    <trust-manager>
      <key-store>
        <key-store-loader>
          <class-name>ssl.CustomKeyStoreLoader</class-name>
          <init-params>
            <init-param>
              <param-type>string</param-type>
              <param-value system-property="coherence.security.client.key">file:trust.jks</param-value>
            </init-param>
          </init-params>
        </key-store-loader>
        <password>password</password>
      </key-store>
    </trust-manager>
  </ssl>
</socket-provider>
