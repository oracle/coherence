<socket-provider>
  <ssl>
    <identity-manager>
      <key system-property="coherence.security.server.encrypted.key"/>
      <cert system-property="coherence.security.server.cert"/>
      <password system-property="coherence.security.server.password"/>
    </identity-manager>
    <trust-manager>
      <cert system-property="coherence.security.client.ca.cert"/>
    </trust-manager>
  </ssl>
</socket-provider>
