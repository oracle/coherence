<socket-provider>
  <ssl>
    <identity-manager>
      <key-loader>
        <class-name>com.tangosol.net.ssl.CustomPrivateKeyLoader</class-name>
        <init-params>
          <init-param>
            <param-type>string</param-type>
            <param-value system-property="coherence.security.client.key"/>
          </init-param>
        </init-params>
      </key-loader>
      <cert-loader>
        <class-name>com.tangosol.net.ssl.CustomCertificateLoader</class-name>
        <init-params>
          <init-param>
            <param-type>string</param-type>
            <param-value system-property="coherence.security.client.cert"/>
          </init-param>
        </init-params>
      </cert-loader>
    </identity-manager>
    <trust-manager>
      <cert-loader>
        <class-name>com.tangosol.net.ssl.CustomCertificateLoader</class-name>
        <init-params>
          <init-param>
            <param-type>string</param-type>
            <param-value system-property="coherence.security.server.ca.cert"/>
          </init-param>
        </init-params>
      </cert-loader>
    </trust-manager>
  </ssl>
</socket-provider>
