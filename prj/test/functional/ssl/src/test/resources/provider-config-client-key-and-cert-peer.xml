<socket-provider>
  <ssl>
    <identity-manager>
      <key system-property="coherence.security.client.key"/>
      <cert system-property="coherence.security.client.cert"/>
    </identity-manager>
    <trust-manager>
      <algorithm>PeerX509</algorithm>
      <cert system-property="coherence.security.client.cert"/>
      <cert system-property="coherence.security.server.cert"/>
    </trust-manager>
  </ssl>
</socket-provider>
