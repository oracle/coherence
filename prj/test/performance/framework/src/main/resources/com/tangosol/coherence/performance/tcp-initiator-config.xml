<initiator-config>
  <tcp-initiator>
    <remote-addresses>
      <socket-address>
        <address>127.0.0.1</address>
        <port>7777</port>
      </socket-address>
    </remote-addresses>
  </tcp-initiator>

  <outgoing-message-handler>
    <request-timeout>60s</request-timeout>
  </outgoing-message-handler>

  <serializer>
    <class-name>com.tangosol.coherence.performance.PofContext</class-name>
  </serializer>
</initiator-config>