  <property name="tde.project"         value="core-gui"/>
  <property name="tde.project.depends" value="core"/>

  <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
  <!-- Project imports                                                     -->
  <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->

  <import file="../../build-import.xml"/>
</project>
